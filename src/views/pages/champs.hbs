<div class="container mt-3">
    <div class="rotation row  ">
        <div class="col-md-5  bg-white m-5 container">
            <p class="h3 text-center">Last champion</p>
            <div class="">
                <img src="http://ddragon.leagueoflegends.com/cdn/img/champion/splash/Viego_0.jpg  " alt="Viego"
                    class="img-thumbnail mb-1">
            </div>
        </div>
        <div class="col-md-5 bg-white text-center m-5 container">
            <p class="h3 ">
                Rotation champs
            </p>
            {{#each rotacion}}
            <a href="/champs/{{this}}">
                <img src="http://ddragon.leagueoflegends.com/cdn/11.2.1/img/champion/{{this}}.png" alt="{{this}}"
                    class="rotation-img m-2 "></a>
            {{/each}}
        </div>
    </div>
    <div class="row">
        {{#each name}}
        <div class="card col-md-auto card-style m-1 text-center">
            <div class="card-title">
                <img src="http://ddragon.leagueoflegends.com/cdn/11.2.1/img/champion/{{this}}.png" alt="{{this}}"
                    class="mt-2">
            </div>
            <div class="card-body">
                <a href="/champs/{{this}}">
                    {{this}}
                </a>
            </div>
        </div>
        {{/each}}
    </div>
</div>