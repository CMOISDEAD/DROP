<div class="container mt-5 bg-white">
    <div class="card p-4 m-4 text-center">
        <div class="card-title">
            {{name}}
        </div>
        <div class="card-body">
            <img src="http://ddragon.leagueoflegends.com/cdn/11.1.1/img/profileicon/{{profileIconId}}.png" alt="#"
                class="summoner-icon">
            <p>
                {{summonerLevel}}
            </p>
        </div>
        <div class="card-footer">
            {{id}}
        </div>
    </div>
</div>